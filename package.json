    "minimist": "^1.2.5",
    "prompts": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "execa": "^4.1.0",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "puppeteer": "^5.5.0",
    "qunit": "^2.11.3",
    "tmp": "^0.2.1",
    "walk-sync": "^2.2.0"
  }
