{
  "name": "@rajasegar/unpack",
  "version": "1.6.1",
  "description": "Create web apps without a bundler",
  "main": "index.js",
  "bin": {
    "unpack": "bin/cli.js"
  },
  "scripts": {
    "start": "bin/cli.js",
    "test": "npm-run-all test:*",
    "test:render": "qunit test/render/**/*.test.js",
    "test:render:cycle": "qunit test/render/cycle.test.js",
    "test:cli": "qunit test/cli/**/*.test.js",
    "lint": "eslint .",
    "deploy": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "unpack",
    "bundler",
    "cdn",
    "esm"
  ],
  "author": "Rajasegar Chandran <rajasegar.c@gmail.com>",
